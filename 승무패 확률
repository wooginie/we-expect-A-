#include <stdio.h>

int main() 
{
// bet:배팅할 팀, opponent: 상대 팀
	char bet[20];
	char opponent[20];
	
	printf("배팅하고 싶은 팀의 이름을 입력하세요: ");
	scanf("%s", bet);
	printf("상대 팀을 입력하세요: ");
	scanf("%s", opponent);
	
	float vic, game, result;	
	game = 10;
	vic = 7;

	result = vic / game;

	printf("%lf", result);

	return 0;
}
